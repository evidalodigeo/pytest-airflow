# exclude .env
# this is excluded because the environment is different in deployment and development
# use the correct environment by creating a symlink, eg. `ln -s .env-dev .env`
.env

# python scaffolding
__pycache__/
.eggs/
build/
dist/
*.egg-info/
*.egg/

# pytest scaffolding
pytestdebug.log

# docs
docs/*.pdf
docs/*.docx

# ignore airflow scaffolding
airflow/*
!airflow/dags

# editor
.vscode/
# idea editor
/.idea/$CACHE_FILE$
/.idea/checkstyle-idea.xml
/.idea/codeStyles/codeStyleConfig.xml
/.idea/compiler.xml
/.idea/misc.xml
/.idea/modules.xml
/.idea/copyright/profiles_settings.xml
/.idea/codeStyles/Project.xml
/pytest-airflow.iml
/.idea/sbt.xml
/.idea/vcs.xml
/.idea/workspace.xml
